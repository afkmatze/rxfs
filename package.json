    "@types/node": "^7.0.13",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "ts-mocha": "^1.0.3",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2"
  },
